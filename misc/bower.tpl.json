{
  "name": "<%= pkg.name %>",
  "version": "<%= pkg.version %>",
  "authors": [
    "<%=pkg.author%>"
  ],
  "description": "<%= pkg.description %>",
  "main": ["js/<%=filename%>-tpls.js"],
  "keywords": [<%= (pkg.keywords.map(function(w) { return '"' + w + '"'; })).join(",") %>],
  "license": "<%= pkg.license %>",
  "homepage": "<%= pkg.homepage %>",
  "private": false,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "^1.2"
  }
}
